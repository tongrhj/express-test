{
  "name": "express-test",
  "version": "1.0.0",
  "description": "",
  "main": "sorter.js",
  "scripts": {
    "start": "node server.js",
    "sorter": "nodemon sorter.js --exec babel-node",
    "scoreboard": "nodemon scoreboard.js --exec babel-node",
    "report": "cat ./coverage/lcov.info | codecov",
    "test": "mocha",
    "test:cover": "babel-node node_modules/.bin/babel-istanbul cover _mocha",
    "codecov": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js"
  },
  "author": "Jared Tong",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-istanbul": "^0.5.8",
    "babel-plugin-syntax-async-functions": "^6.1.18",
    "babel-plugin-transform-regenerator": "^6.2.0",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "codecov.io": "^0.1.6",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "nodemon": "^1.8.1",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "node-fetch": "^1.3.3"
  }
}
